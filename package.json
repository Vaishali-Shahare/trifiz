{
  "name": "trifiz",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "node server",
    "dev-build": "node server -b",
    "html-lint": "htmlhint ./src --config ./.htmlhintrc || exit 0;",
    "html-lint:pr": "node ./ui-tasks/htmlhint-api",
    "prehtml-lint:build": "node ./ui-tasks/node-fs-extra-cli -fse=ef -path1=./tmp/checkstyle/chekstyle-html.xml",
    "html-lint:build": "htmlhint ./src --config ./.htmlhintrc -f checkstyle > ./tmp/checkstyle/chekstyle-html.xml || exit 0;",
    "css-lint": "stylelint ./src/**/*.{css,scss} --cache --ignore-pattern lib || exit 0;",
    "css-lint:pr": "stylelint ./src/**/*.{css,scss} --ignore-path ./config/.stylelintignore",
    "precss-lint:build": "node ./ui-tasks/node-fs-extra-cli -fse=ef -path1=./tmp/checkstyle/checkstyle-css.xml",
    "css-lint:build": "stylelint ./src/**/*.{css,scss} --ignore-pattern lib --custom-formatter=./node_modules/stylelint-checkstyle-formatter > ./tmp/checkstyle/checkstyle-css.xml || exit 0;",
    "js-lint": "eslint ./src --ignore-pattern lib --cache --quiet || exit 0;",
    "js-lint:pr": "eslint ./src --ignore-path ./config/.eslintignore",
    "js-lint:build": "eslint ./src --ignore-pattern lib -f checkstyle -o ./tmp/checkstyle/checkstyle-js.xml--quiet || exit 0;",
    "precommit": "lint-staged",
    "pr-check": "npm run html-lint:pr && npm run css-lint:pr && npm run js-lint:pr",
    "build-dashboard": "webpack --config ./config/webpack/dashboard/webpack.config.dashboard.prod.js",
    "build": "node ./ui-tasks/prebuild && npm run build-dashboard"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vaishali-Shahare/trifiz.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "angular": "1.6.9",
    "angular-mocks": "1.6.9",
    "angular-resource": "1.6.9",
    "angular-route": "1.6.9",
    "angular-sanitize": "1.6.9",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "4.0.0",
    "bootstrap-less": "^3.3.8",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-angular": "3.3.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-rulesdir": "0.1.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-saver": "1.3.8",
    "font-awesome": "^4.7.0",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "htmlhint": "0.9.13",
    "jasmine-core": "3.1.0",
    "jquery": "3.3.1",
    "karma": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "1.1.1",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.5",
    "moment": "2.22.0",
    "ngtemplate-loader": "2.0.1",
    "node-sass": "4.8.3",
    "null-loader": "0.1.1",
    "prettier": "1.11.1",
    "sass-loader": "6.0.7",
    "stylelint": "9.2.0",
    "stylelint-checkstyle-formatter": "0.1.1",
    "stylelint-config-standard": "18.2.0",
    "webpack": "^4.6.0",
    "xlsx": "0.12.8"
  },
  "optionalDependencies": {
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "event-source-polyfill": "0.0.12",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.18.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "node-notifier": "^5.2.1",
    "request": "^2.85.0",
    "style-loader": "^0.20.3",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.21.2",
    "xml2js": "^0.4.19"
  },
  "lint-staged": {
    "*.js": [
      "prettier --ignore-path ./config/.eslintignore --write",
      "git add"
    ]
  },
  "dependencies": {
    "popper.js": "^1.14.3",
    "webpack-cli": "^2.0.14"
  }
}
