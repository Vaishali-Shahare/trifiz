{
	"extends": "stylelint-config-standard",
	"rules": {
		"indentation": 4,
		"selector-type-no-unknown": [
			true,
			{
				"ignore": [
					"custom-elements"
				]
			}
		],
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": [
					"include", "mixin", "extends", "function", "if"
				]
			}
		]
	}
}